{
    "name": "roadiz/skeleton",
    "type": "project",
    "license": "mit",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "nelmio/cors-bundle": "^2.2",
        "roadiz/cms-pack": "~2.1.0 || dev-develop",
        "sentry/sentry-symfony": "^4.2",
        "symfony/flex": "*"
    },
    "require-dev": {
        "blackfire/php-sdk": "^1.28",
        "phpstan/phpstan": "^1.5.3",
        "phpstan/phpstan-symfony": "^1.1.8",
        "phpstan/phpstan-doctrine": "^1.3.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "roadiz/compat-bundle": "dev-develop",
        "roadiz/core-bundle": "dev-develop",
        "roadiz/rozier-bundle": "dev-develop",
        "rezozero/intervention-request-bundle": "dev-develop",
        "ambroisemaupate/intervention-request": "dev-develop",
        "roadiz/documents": "dev-develop",
        "roadiz/entity-generator": "dev-develop",
        "roadiz/models": "~2.1.0 || dev-develop",
        "roadiz/rozier": "~2.1.0 || dev-develop"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "themes:assets:install Rozier": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*",
            "docker": false
        }
    }
}
